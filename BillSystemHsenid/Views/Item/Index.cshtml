@model List<BillSystemHsenid.Models.DefineItems>
@{
    ViewData["Title"] = "Item List";
   


}

<h2>Items List</h2>
<a asp-action="Create" class="btn btn-primary">Add New Item</a>
<table class="table mt-3">
    <thead>
        <tr>
            <th>Item Name</th>
            <th>Price</th>
            <th>Quantity</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ItemName</td>
                <td>@item.Price</td>
                <td>@item.AvailableQuantity</td>
            </tr>
        }
    </tbody>
</table>